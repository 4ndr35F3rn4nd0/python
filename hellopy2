# App compatible con Python 2

# Eliminamos el uso de 'typing' ya que no es compatible con Python 2

def saludar(nombres):
    # Usamos un bucle para iterar sobre la lista de nombres
    for nombre in nombres:
        print "Hola, {} ðŸ‘‹".format(nombre)  # Usamos .format() en lugar de f-string

def main():
    nombres = ["Red Hat", "Enablement", "Andres", "Python 2"]
    saludar(nombres)

if __name__ == "__main__":
    main()
